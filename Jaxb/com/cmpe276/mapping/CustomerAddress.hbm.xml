<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping schema="SalesLT">
	<class name="com.cmpe276.dbase.CustomerAddressType" table="CustomerAddress">
    
        <id name="CustomerID">
            <generator class="native"/>
        </id>
		 
        <property name="AddressID" column="AddressID" />
    	<property name="AddressType" column="AddressType" />
	
    </class>	
</hibernate-mapping>       