<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping schema="SalesLT">
	<class name="com.cmpe276.dbase.ProductCategoryType" table="ProductCategory">
    
        <id name="ProductCategoryID">
            <generator class="native"/>
        </id>
		 
        <property name="ParentProductCategoryID" column="ParentProductCategoryID" />
    	<property name="Name" column="Name"  />
		
    </class>	
</hibernate-mapping>       